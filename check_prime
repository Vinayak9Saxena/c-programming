#include <stdio.h>

void main()
{
    int i, num, count=0;
    printf("Enter the Number to check if its prime or not: ");
    scanf("%d", &num);
    
    for(i = 2; i<num; i++)
    {
        if(num%i==0)
        {
            count++;
            break;
        }
    }
    if(count == 0)
    {
        printf("The number entered is Prime");
    }
    else{
        printf("The number entered is not Prime");
    }
    
}
