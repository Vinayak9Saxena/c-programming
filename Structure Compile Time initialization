# include <stdio.h>
void main()
{
    struct student
    {
        int roll;
        // char name[30];
        int age;
    };
    //compile time Initialization of structure variable
    struct student s1={51, 21};
    struct student s2={51, 21};
    struct student s3={52, 21};
    
    //Structure variables can also be comapared
    if((s1.roll==s2.roll)&&(s1.age==s2.age))
    {
        printf("Both Structure variables are same");
    }
    else
    {
        printf("No they are not same");
    }
}
