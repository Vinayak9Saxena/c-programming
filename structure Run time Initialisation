# include<stdio.h>
void main()
{
    struct student
    {
        int roll;
        char name[30];
        int age;
    };
    struct student s1, s2, s3;
    printf("Enter record of first student\n");
    scanf("%d %s %d", &s1.roll, &s1.name, &s1.age);
    printf("Enter record of third student");
    scanf("%d %s %d", &s3.roll, &s3.name, &s3.age);
    
    //copying the content of One structure variable into another.
    s2=s1;
    printf("the record of second student is:\n%d \n%s \n%d", s2.roll, s2.name, s2.age);
}
